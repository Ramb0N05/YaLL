<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net5.0;netcoreapp3.1;netcoreapp2.1;netstandard2.0;netstandard2.1;net48;net47;net46;net45;net40</TargetFrameworks>
    <Authors>RamboNo5</Authors>
    <Company>SharpRambo</Company>
    <Copyright></Copyright>
    <PackageLicenseExpression>GPL-3.0-only</PackageLicenseExpression>
    <NeutralLanguage>en-150</NeutralLanguage>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Yet another Logger Library</Description>
    <Version>1.0.0-gpl</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ExtensionsLib" Version="1.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>
</Project>
